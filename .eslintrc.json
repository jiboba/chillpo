{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "off",
    "no-multi-spaces": "off",
    "no-new": "off",
    "one-var": "off",
    "quote-props": "off"
  }
}
